{
  "name": "@sentry/profiler-node",
  "version": "0.0.0",
  "description": "Sampling based nodejs profiler",
  "main": "./lib/index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "lib/*",
    "build/release/cpu_profiler.node"
  ],
  "scripts": {
    "postinstall": "npm run build:bindings",
    "clean": "rm -rf ./lib && rm -rf build",
    "lint": "eslint ./src --ext .ts",
    "build": "npm run build:bindings && npm run build:lib",
    "build:lib": "tsc",
    "build:bindings": "node-gyp build",
    "build:benchmark:format": "node-gyp -DFORMAT_BENCHMARK=1 rebuild",
    "benchmark": "npm run benchmark:methods && npm run benchmark:profiler && npm run benchmark:server && npm run benchmark:format",
    "benchmark:methods": "node benchmarks/cpu/benchmark.methods.js",
    "benchmark:profiler": "node benchmarks/cpu/benchmark.profiler.js",
    "benchmark:server": "node benchmarks/cpu/benchmark.server.js",
    "benchmark:format": "yarn build:benchmark:format && node benchmarks/format/benchmark.format.js",
    "test:watch": "jest --watch",
    "test": "jest",
    "prettier": "prettier --config ./.prettierrc --write"
  },
  "keywords": [
    "profiling",
    "monitoring",
    "cpu",
    "node"
  ],
  "author": "jonas.badalic@sentry.io",
  "license": "ISC",
  "dependencies": {
    "@sentry/hub": "^7.10.0",
    "@sentry/node": "^7.10.0",
    "@sentry/tracing": "^7.12.1",
    "@sentry/types": "^7.8.1",
    "@sentry/utils": "^7.10.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.2",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "autocannon": "^7.9.0",
    "benchmark": "^2.1.4",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.1",
    "jest": "^28.1.3",
    "nan": "^2.16.0",
    "node-gyp": "^9.0.0",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simple-zstd": "^1.4.0",
    "sqlite3": "^5.0.8",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
